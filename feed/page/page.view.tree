$conduit_feed_page $mol_page
	title @ \Articles
	minimal_width 400
	tools /
		<= Add_link $mol_link
			arg *
				article \
				edit \
			sub / <= Add_icon $mol_icon_plus
	body /
		<= Article_links $mol_list
			rows <= article_links /
	Article_link!index $mol_link
		arg *
			article <= article_slug!index \
		sub /
			<= Article_card!index $conduit_article_card
				article <= article!index $conduit_article
